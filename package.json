{
  "name": "@hyperledgendary/weftility",
  "version": "0.0.6",
  "description": "Simple CLI for testing transactions",
  "bin": {
    "weft": "lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "npm run format && npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty && npm link",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledgendary/weftility"
  },
  "keywords": [
    "fabric-ledger",
    "Hyperledger Fabric",
    "Fabric Ledger"
  ],
  "author": "",
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.11",
    "@types/rimraf": "^3.0.0",
    "@types/yargs": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.5",
    "chalk": "^4.1.0",
    "cliffy": "^2.3.0",
    "fabric-ca-client": "^2.2.1",
    "fabric-network": "^2.2.0",
    "fabric-wallet-migration": "^0.2.0",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "yargs": "^15.1.0"
  },
  "engines": {
    "node": ">=12.15.0"
  }
}
